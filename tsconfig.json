{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es2015", "es2016", "dom"],
    "module": "esnext",
    // "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    // "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react"
  },
  "include": [
    "./client"
  ],

}
